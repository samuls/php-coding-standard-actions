{
    "name": "samuls/php-coding-standard-actions",
    "description": "Support tools for check php coding standards.",
    "type": "composer-plugin",
    "license": "MIT",
    "scripts": {
        "lint": "phplint --no-cache src/",
        "phpcs": "phpcs --standard=PSR12 src/ tests/",
        "phpcbf": "phpcbf --standard=PSR12 src/ tests/",
        "test": "phpunit tests/"
    },
    "require": {
    "php": "8.1.2"
    }
}
